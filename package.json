  "description": "fingo中台管理系统脚手架",
  "author": "weidong",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env FINGO_APP_ENV=dev umi build",
    "build:test": "cross-env FINGO_APP_ENV=test umi build",
    "build:prd": "cross-env FINGO_APP_ENV=prd umi build",
    "dev": "cross-env FINGO_APP_ENV=dev MOCK=none umi dev",
    "start": "cross-env FINGO_APP_ENV=dev umi dev",
    "test": "cross-env FINGO_APP_ENV=test MOCK=none umi dev",
    "prd": "cross-env FINGO_APP_ENV=prd MOCK=none umi dev",
    "proto": "pbjs -t json-module -w commonjs -o src/proto/proto.js src/proto/common/*.proto src/proto/boss/*.proto src/proto/erp/*.proto"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
